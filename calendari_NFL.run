# Borrar los anterior comandos en AMPL
reset;

# Cargar el modelo
model /home/bernat/Desktop/Practica-2-PM/calendari_NFL.mod;

# Cargar los datos
data /home/bernat/Desktop/Practica-2-PM/calendari_NFL.dat;

# Indicadi√≥n a ampl de que se quiere utilizar como solver: CPLEX
option solver cplex;

